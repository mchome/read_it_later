{
    "manifest_version": 2,

    "name": "read_it_later",
    "version": "2.0",
    "description": "Save the all tabs for next browsing.",

    "browser_action": {
        "default_popup": "static/html/index.html",
        "default_title": "Read it later"
    },
    "permissions": ["tabs", "storage", "chrome://favicon/"]
}